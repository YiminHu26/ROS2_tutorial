<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="my_robot_2">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <!-- Import my_robot_2 urdf file -->
    <xacro:include filename="$(find my_robot_2_description)/urdf/my_robot_2.urdf.xacro" />

    <!-- Import control_xacro -->
    <xacro:include filename="my_robot_2.ros2_control.xacro" />


    <xacro:my_robot_2_ros2_control name="FakeSystem" initial_positions_file="$(arg initial_positions_file)"/>

</robot>
